{
  "name": "stryber/laravel-log-handler",
  "description": "Stryber Laravel Log Handler for Laravel Framework",
  "minimum-stability": "stable",
  "license": "proprietary",
  "authors": [
    {
      "name": "Naidenko Dmytro",
      "email": "dmitryi@stryber.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "graylog2/gelf-php": "^1.6",
    "illuminate/contracts": "^8.0",
    "illuminate/log": "^8.0",
    "ramsey/uuid": "^4.0",
    "stryber/laravel-uuid-helper": "^8.0",
    "illuminate/http": "^8.0",
    "psr/log": "^1.1"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "vimeo/psalm": "^4.6"
  },
  "autoload": {
    "psr-4": {
      "Stryber\\Logger\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Stryber\\Logger\\LoggerServiceProvider"
      ]
    }
  }
}
